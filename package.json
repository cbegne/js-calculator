{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all --parallel start:parcel watch:sass",
    "start:parcel": "parcel index.html --open",
    "build": "parcel build index.html",
    "watch:sass": "node-sass -w src/styles.scss -o css"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.9.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "sass": "^1.26.5"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
